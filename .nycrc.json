{
  "all": true,
  "exclude": [
    "node_modules",
    "**/specs/**/*.spec.ts"
  ],
  "extension": [
    ".ts"
  ],
  "include": [
    "packages/*/src/**/*.ts"
  ],
  "instrument": true,
  "report-dir": ".cache/coverage",
  "reporter": "text",
  "sourceMap": true,
  "temp-directory": ".cache/nyc"
}
